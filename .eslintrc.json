{
    "extends": "airbnb",
    "globals": {
        "fetch": false
    },
    "rules": {
        "no-alert": 0,
        "no-undef": 0,
        "no-comments": 0,
        "import/no-named-as-default": 0,
        "import/no-named-as-default-member": 0
    },
    "parserOptions": {
        "ecmaVersion": 11
    }
}